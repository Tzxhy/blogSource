   const ret: Record<string, string[]> = {}\n    pairs.forEach(p => {\n        const pair = p.split('\t')\n        if (pair[0] in ret) {\n            ret[pair[0]].push(pair[1])\n        } else {\n            ret[pair[0]] = [pair[1]]\n        }\n    })\n    let retStr = []\n    for (const k in ret) {\n        if (ret[k].length === 1 && ret[k][0] === k) { // 直辖市\n            retStr.push(k)\n        } else {\n            retStr.push(`${k}（${ret[k].map(i => `${i}市`).join('；')}）`)\n        }\n    }\n    return retStr.join('，')\n}\nconst layout = {\n    labelCol: { span: 8 },\n    wrapperCol: { span: 16 },\n};\nconst tailLayout = {\n    wrapperCol: { offset: 8, span: 16 },\n};\nexport default function Tool2() {\n    const v = useRef('')\n    const [s, ss] = useState('')\n    const [lines, updateLines] = useState(0)\n    const [form] = Form.useForm();\n    const resultRef = useRef<HTMLParagraphElement>(null);\n\n    function changeValue(e: ChangeEvent<HTMLTextAreaElement>) {\n        v.current = e.target.value\n        updateLines(v.current.split('\\n').length)\n    }\n    function compute() {\n        if (!v.current) return\n        ss(split(v.current))\n    }\n\n    function copy() {\n        window.getSelection()!.removeAllRanges()\n\n        const range = document.createRange()\n        \n        range.selectNode(resultRef.current as HTMLParagraphElement)\n        window.getSelection()!.addRange(range)\n\n        const msg = document.execCommand('copy')\n        if (msg) {\n            message.success('成功拷贝数据到剪切板')\n        }\n\n        window.getSelection()!.removeAllRanges()\n    }\n\n    function clear() {\n        form.setFieldsValue({\n            originData: '',\n        })\n        updateLines(0)\n        ss('')\n    }\n\n    return (\n        <Layout style={{display: 'flex', flexDirection: 'column', height: '100%'}}>\n            <h1 style={{fontSize: 32}}>省市合并</h1>\n            <Divider />\n            <Layout style={{marginTop: 50, display: 'flex', flexDirection: 'row', alignItems: 'stretch'}}>\n                <Content style={{flex: 1}}>\n                    <h2 style={{marginBottom: 40, textAlign: 'center', fontSize: 28}}>一、数据输入</h2>\n                    <Form\n                        form={form}\n                        {...layout}\n                        name=\"basic\"\n                        initialValues={{ remember: true }}\n                    >\n                        <Form.Item\n                            label=\"1-输入数据\"\n                            name=\"originData\"\n                            rules={[{ required: true, message: '请输入原始数据' }]}\n                            extra={lines ? <p style={{marginTop: 10,}}>当前数据有 {lines} 行。</p> : null}\n                        >\n                            <Input.TextArea style={{width: 200}} rows={6} onChange={changeValue} />\n                        </Form.Item>\n                        \n                        <Form.Item {...tailLayout}>\n                            <Button type=\"primary\" onClick={compute}>\n                            2-点击按钮\n                            </Button>\n                        </Form.Item>\n                    </Form>\n                    \n                    \n                </Content>\n                <div style={{marginLeft: 16, marginRight: 16, width: 1, backgroundColor: '#e8e8e8'}} />\n                <Content style={{flex: 1}}>\n                    <h2 style={{marginBottom: 40, textAlign: 'center', fontSize: 28}}>二、结果输出</h2>\n                    <Space direction=\"vertical\" style={{width: '100%'}}>\n                        \n                        {\n                            s ? <div style={{padding: 16, backgroundColor: 'white', borderRadius: 8,}}>\n                                <p ref={resultRef}>{s}</p>\n                                <Space size=\"large\" style={{display: 'flex', justifyContent: 'center'}}>\n                                    <Button type=\"primary\" onClick={copy}>拷贝数据</Button>\n                                    <Button type=\"default\" onClick={clear}>清空数据</Button>\n                                </Space>\n                            </div> : <div style={{display: 'flex', flexDirection: 'column', alignItems: 'center'}}>\n                                <Empty description='木有数据' image={Empty.PRESENTED_IMAGE_SIMPLE} />\n                                <p> {'<=='}   请先在左侧录入数据~</p>\n                            </div>\n                        }\n                    </Space>\n                </Content>\n            </Layout>\n            \n            {/* <Sider style={{marginLeft: 20, width: 100, backgroundColor: 'white', padding: 16,}}>\n                <p>\n                    示例数据：\n                </p>\n                <p style={{whiteSpace: 'pre-wrap', height: 300, overflow: 'auto'}}>{tip}</p>\n            </Sider> */}\n        </Layout>\n    )\n}\n","/Users/tanzhixuan/Desktop/study/doc/blogSource/web-tools/src/constants/key.ts",[],"/Users/tanzhixuan/Desktop/study/doc/blogSource/web-tools/src/pages/getTypes/index.tsx",[],"/Users/tanzhixuan/Desktop/study/doc/blogSource/web-tools/src/services/getMostTypes.ts",[],"/Users/tanzhixuan/Desktop/study/doc/blogSource/web-tools/src/services/url.ts",[],["54","55"],"/Users/tanzhixuan/Desktop/study/doc/blogSource/web-tools/src/pages/tool2/tips.ts",[],{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","replacedBy":"59"},{"ruleId":"60","severity":1,"message":"61","line":4,"column":5,"nodeType":"62","messageId":"63","endLine":4,"endColumn":21},{"ruleId":"60","severity":1,"message":"64","line":5,"column":5,"nodeType":"62","messageId":"63","endLine":5,"endColumn":23},{"ruleId":"60","severity":1,"message":"65","line":6,"column":5,"nodeType":"62","messageId":"63","endLine":6,"endColumn":21},{"ruleId":"60","severity":1,"message":"66","line":8,"column":5,"nodeType":"62","messageId":"63","endLine":8,"endColumn":20},{"ruleId":"60","severity":1,"message":"67","line":9,"column":5,"nodeType":"62","messageId":"63","endLine":9,